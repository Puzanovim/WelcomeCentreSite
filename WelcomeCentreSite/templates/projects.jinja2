{% extends 'layout.jinja2' %}

{% block title %}{{ title }}{% endblock %}

{% block page_content %}
    <div class="page-header">
        <h1>Проекты Welcome Центра УрФУ</h1>
    </div>
<div class="row  content_row">
{% for project in projects %}
    <div class="col-md-3 col-md-offset-2 project">
        <p>{{ project.name }}</p>
        <p>{{ project.description }}</p>
        <p>{{ project.email }}</p>
        <p>{{ project.phone }}</p>
        <p>{{ project.contact_face }}</p>
    </div>
{% endfor %}
</div>
{% endblock %}